#################################################################
#    Object Properties
#################################################################

###  http://purl.org/ontology/onfraud#afectaA
<http://purl.org/ontology/onfraud#afectaA> rdf:type owl:ObjectProperty ;
                                           rdfs:subPropertyOf owl:topObjectProperty ;
                                           rdfs:domain <http://purl.org/ontology/onfraud#PerjuicioEconómico> ;
                                           rdfs:range <http://xmlns.com/foaf/0.1/Agent> .


###  http://purl.org/ontology/onfraud#afectaAVictima
<http://purl.org/ontology/onfraud#afectaAVictima> rdf:type owl:ObjectProperty ;
                                                  rdfs:subPropertyOf <http://purl.org/ontology/onfraud#afectaA> ;
                                                  rdfs:domain <http://purl.org/ontology/onfraud#PerjuicioEconómico> ;
                                                  rdfs:range <http://purl.org/ontology/onfraud#Victima> .


###  http://purl.org/ontology/onfraud#asignadoA
<http://purl.org/ontology/onfraud#asignadoA> rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf <http://www.w3.org/ns/org#memberOf> ;
                                             rdfs:domain <http://purl.org/ontology/onfraud#Empleado> ;
                                             rdfs:range <http://purl.org/ontology/onfraud#ProcedimientoForense> .


###  http://purl.org/ontology/onfraud#calculaPerjuicioEconomico
<http://purl.org/ontology/onfraud#calculaPerjuicioEconomico> rdf:type owl:ObjectProperty ;
                                                             rdfs:subPropertyOf <http://purl.org/ontology/onfraud#realizaProcedimientoForense> ;
                                                             rdfs:domain <http://purl.org/ontology/onfraud#EquipoForensic> ;
                                                             rdfs:range <http://purl.org/ontology/onfraud#PerjuicioEconómico> .


###  http://purl.org/ontology/onfraud#causaPerjuicioEconomico
<http://purl.org/ontology/onfraud#causaPerjuicioEconomico> rdf:type owl:ObjectProperty ;
                                                           rdfs:domain <http://purl.org/ontology/onfraud#Fraude> ;
                                                           rdfs:range <http://purl.org/ontology/onfraud#PerjuicioEconómico> .


###  http://purl.org/ontology/onfraud#cometeFraude
<http://purl.org/ontology/onfraud#cometeFraude> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf owl:topObjectProperty ;
                                                rdfs:domain <http://purl.org/ontology/onfraud#Defraudador> ;
                                                rdfs:range <http://purl.org/ontology/onfraud#Fraude> .


###  http://purl.org/ontology/onfraud#cometeFraudeInterno
<http://purl.org/ontology/onfraud#cometeFraudeInterno> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://purl.org/ontology/onfraud#cometeFraude> ;
                                                       rdfs:domain <http://purl.org/ontology/onfraud#DefraudadorInterno> ;
                                                       rdfs:range <http://purl.org/ontology/onfraud#FraudeInterno> .


###  http://purl.org/ontology/onfraud#estaAsociadoCon
<http://purl.org/ontology/onfraud#estaAsociadoCon> rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf owl:topObjectProperty ;
                                                   rdfs:domain <http://purl.org/ontology/onfraud#Redflag> ;
                                                   rdfs:range <http://purl.org/ontology/onfraud#Fraude> .


###  http://purl.org/ontology/onfraud#identifica
<http://purl.org/ontology/onfraud#identifica> rdf:type owl:ObjectProperty ;
                                              rdfs:domain <http://purl.org/ontology/onfraud#EquipoForensic> ;
                                              rdfs:range <http://purl.org/ontology/onfraud#DefraudadorInterno> .


###  http://purl.org/ontology/onfraud#realizaInvestigacion
<http://purl.org/ontology/onfraud#realizaInvestigacion> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <http://purl.org/ontology/onfraud#realizaProcedimientoForense> ;
                                                        rdfs:domain <http://purl.org/ontology/onfraud#EquipoForensic> ;
                                                        rdfs:range <http://purl.org/ontology/onfraud#Investigacion> .


###  http://purl.org/ontology/onfraud#realizaProcedimientoForense
<http://purl.org/ontology/onfraud#realizaProcedimientoForense> rdf:type owl:ObjectProperty ;
                                                               rdfs:subPropertyOf owl:topObjectProperty ;
                                                               rdfs:domain <http://purl.org/ontology/onfraud#EquipoForensic> ;
                                                               rdfs:range <http://purl.org/ontology/onfraud#ProcedimientoForense> .


###  http://purl.org/ontology/onfraud#worksIn
<http://purl.org/ontology/onfraud#worksIn> rdf:type owl:ObjectProperty ;
                                           rdfs:subPropertyOf <http://www.w3.org/ns/org#memberOf> ;
                                           rdfs:domain <http://purl.org/ontology/onfraud#Empleado> ;
                                           rdfs:range <http://www.w3.org/ns/org#FormalOrganization> .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdfs:domain <http://purl.org/ontology/onfraud#Redflag> ;
                      rdfs:range <http://purl.org/ontology/onfraud#Fraude> .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/ontology/onfraud#descripcion
<http://purl.org/ontology/onfraud#descripcion> rdf:type owl:DatatypeProperty ;
                                               rdfs:subPropertyOf owl:topDataProperty ;
                                               rdfs:domain <http://purl.org/ontology/onfraud#Fraude> ;
                                               rdfs:range xsd:string .


###  http://purl.org/ontology/onfraud#descripcionRedFlag
<http://purl.org/ontology/onfraud#descripcionRedFlag> rdf:type owl:DatatypeProperty ;
                                                      rdfs:subPropertyOf owl:topDataProperty ;
                                                      rdfs:domain <http://purl.org/ontology/onfraud#Redflag> ;
                                                      rdfs:range xsd:string .


###  http://purl.org/ontology/onfraud#idFraude
<http://purl.org/ontology/onfraud#idFraude> rdf:type owl:DatatypeProperty ;
                                            rdfs:subPropertyOf owl:topDataProperty ;
                                            rdfs:domain <http://purl.org/ontology/onfraud#Fraude> ;
                                            rdfs:range xsd:string .


###  http://purl.org/ontology/onfraud#perjuicio
<http://purl.org/ontology/onfraud#perjuicio> rdf:type owl:DatatypeProperty ;
                                             rdfs:subPropertyOf owl:topDataProperty ;
                                             rdfs:domain <http://purl.org/ontology/onfraud#Fraude> ;
                                             rdfs:range owl:real .


###  http://www.w3.org/2002/07/owl#topDataProperty
owl:topDataProperty rdfs:domain <http://purl.org/ontology/onfraud#Redflag> ;
                    rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Activity
<http://dbpedia.org/ontology/Activity> rdf:type owl:Class ;
                                       owl:equivalentClass <http://www.w3.org/ns/prov#Activity> .


###  http://dbpedia.org/ontology/Employer
<http://dbpedia.org/ontology/Employer> rdf:type owl:Class ;
                                       owl:equivalentClass <http://xmlns.com/foaf/0.1/Organization> .


###  http://dbpedia.org/ontology/Person
<http://dbpedia.org/ontology/Person> rdf:type owl:Class ;
                                     owl:equivalentClass <http://www.w3.org/2000/10/swap/pim/contact#Person> .


###  http://dbpedia.org/ontology/Work
<http://dbpedia.org/ontology/Work> rdf:type owl:Class ;
                                   rdfs:subClassOf <http://www.w3.org/ns/prov#Activity> .


###  http://dbpedia.org/resource/Conflict_of_Interest
<http://dbpedia.org/resource/Conflict_of_Interest> rdf:type owl:Class ;
                                                   owl:equivalentClass <http://purl.org/ontology/onfraud#ConflictoDeInteres> ;
                                                   rdfs:subClassOf <http://purl.org/ontology/onfraud#Corrupción> .


###  http://dbpedia.org/resource/Corruption
<http://dbpedia.org/resource/Corruption> rdf:type owl:Class ;
                                         owl:equivalentClass <http://purl.org/ontology/onfraud#Corrupción> ;
                                         rdfs:subClassOf <http://purl.org/ontology/onfraud#FraudeInterno> .


###  http://dbpedia.org/resource/Employee
<http://dbpedia.org/resource/Employee> rdf:type owl:Class ;
                                       owl:equivalentClass <http://purl.org/ontology/onfraud#Empleado> .


###  http://dbpedia.org/resource/Extortion
<http://dbpedia.org/resource/Extortion> rdf:type owl:Class ;
                                        owl:equivalentClass <http://purl.org/ontology/onfraud#Extorsión> ;
                                        rdfs:subClassOf <http://purl.org/ontology/onfraud#Corrupción> .


###  http://dbpedia.org/resource/Fraud
<http://dbpedia.org/resource/Fraud> rdf:type owl:Class ;
                                    owl:equivalentClass <http://purl.org/ontology/onfraud#Fraude> ;
                                    rdfs:subClassOf <http://www.w3.org/ns/prov#Activity> .


###  http://dbpedia.org/resource/Fraudster
<http://dbpedia.org/resource/Fraudster> rdf:type owl:Class ;
                                        owl:equivalentClass <http://purl.org/ontology/onfraud#Defraudador> .


###  http://dbpedia.org/resource/Larceny
<http://dbpedia.org/resource/Larceny> rdf:type owl:Class ;
                                      owl:equivalentClass <http://purl.org/ontology/onfraud#ApropiacionDeActivos> ;
                                      rdfs:subClassOf <http://purl.org/ontology/onfraud#FraudeInterno> .


###  http://dbpedia.org/resource/Theft
<http://dbpedia.org/resource/Theft> rdf:type owl:Class ;
                                    owl:equivalentClass <http://purl.org/ontology/onfraud#Robo> ;
                                    rdfs:subClassOf <http://www.w3.org/ns/prov#Activity> .


###  http://dbpedia.org/resource/bribery
<http://dbpedia.org/resource/bribery> rdf:type owl:Class ;
                                      owl:equivalentClass <http://purl.org/ontology/onfraud#Soborno> ;
                                      rdfs:subClassOf <http://purl.org/ontology/onfraud#Corrupción> .


###  http://dbpedia.org/resource/Skimming_(fraud)
<http://dbpedia.org/resource/Skimming_(fraud)> rdf:type owl:Class ;
                                               owl:equivalentClass <http://purl.org/ontology/onfraud#Skimming> ;
                                               rdfs:subClassOf <http://purl.org/ontology/onfraud#RoboDeposito> .


###  http://purl.org/ontology/onfraud#AlteraciónCheque
<http://purl.org/ontology/onfraud#AlteraciónCheque> rdf:type owl:Class ;
                                                    rdfs:subClassOf <http://purl.org/ontology/onfraud#PagosFraudulentos> ;
                                                    rdfs:comment "A scheme in which a person steals his or her employer’s funds by intercepting, forging or altering a check or electronic payment drawn on one of the organization’s bank accounts."@en ,
                                                                 "Se produce cuando el defraudador roba fondos de la organización interceptando, falsificando o alterando un cheque o pago electrónico de una de las cuentas bancarias de la organización"@es ;
                                                    rdfs:label "Alteración de cheques"@es ,
                                                               "Check Tampering"@en .


###  http://purl.org/ontology/onfraud#AnulacionFalsa
<http://purl.org/ontology/onfraud#AnulacionFalsa> rdf:type owl:Class ;
                                                  rdfs:subClassOf <http://purl.org/ontology/onfraud#PagosFraudulentosRegistroDesembolso> ;
                                                  owl:disjointWith <http://purl.org/ontology/onfraud#DevolucionFalsa> ;
                                                  rdfs:comment "Está relacionado con las devoluciones de mercancías en las que un defraudador guarda una copia del recibo del cliente en el momento de la venta y luego lo utiliza para crear un vacío ficticio (es decir, como si el cliente devolviera la mercancía)."@es ,
                                                               "This scheme is related to merchandise returns in which a a fraud perpetrator keeps a copy of the customer’s receipt at the time of sale and later uses it to create a fictitious void (i.e., as if the customer returned the merchandise)."@en ;
                                                  rdfs:label "Anulaciones falsas"@es ,
                                                             "False Void"@en .


###  http://purl.org/ontology/onfraud#ApropiacionDeActivos
<http://purl.org/ontology/onfraud#ApropiacionDeActivos> rdf:type owl:Class ;
                                                        rdfs:subClassOf <http://purl.org/ontology/onfraud#FraudeInterno> ;
                                                        rdfs:comment "Asset misappropriation fraud happens when people who are entrusted to manage the assets of an organisation steal from it or misuses these assets."@en ,
                                                                     "Consiste en apropiarse de activos financieros o no financieros de la organización, bien robándolos, bien realizando un uso indebido de éstos"@es ;
                                                        rdfs:label "Apropiación de activos"@es ,
                                                                   "Asset Misappropriation"@en .


###  http://purl.org/ontology/onfraud#ApropiaciónDeActivosDeNaturalezaFinanciera
<http://purl.org/ontology/onfraud#ApropiaciónDeActivosDeNaturalezaFinanciera> rdf:type owl:Class ;
                                                                              rdfs:subClassOf <http://purl.org/ontology/onfraud#ApropiacionDeActivos> ;
                                                                              owl:disjointWith <http://purl.org/ontology/onfraud#ApropiaciónDeActivosDeNaturalezaNoFinanciera> ;
                                                                              rdfs:comment "Both Cash and financial asset missapropriation schemes"@en ,
                                                                                           "Esquemas de apropiación de activos financieros o monetarios"@es ;
                                                                              rdfs:label "Apropiación de fondos"@es ,
                                                                                         "Asset Misappropriation of Cash"@en .


###  http://purl.org/ontology/onfraud#ApropiaciónDeActivosDeNaturalezaNoFinanciera
<http://purl.org/ontology/onfraud#ApropiaciónDeActivosDeNaturalezaNoFinanciera> rdf:type owl:Class ;
                                                                                rdfs:subClassOf <http://purl.org/ontology/onfraud#ApropiacionDeActivos> ;
                                                                                rdfs:comment "Esquemas de apropiación de activos no financieros de la organización"@es ,
                                                                                             "Non financial asset missapropriation schemes"@en ;
                                                                                rdfs:label "Apropiación de Activos No Financieros"@en ,
                                                                                           "Non-Cash Asset Missapropriation"@en .


###  http://purl.org/ontology/onfraud#CompraPersonal
<http://purl.org/ontology/onfraud#CompraPersonal> rdf:type owl:Class ;
                                                  rdfs:subClassOf <http://purl.org/ontology/onfraud#PagosFraudulentosEsquemaFacturación> ;
                                                  rdfs:comment "Ocurre cuando un empleado pasa una factura a la empresa para que realice el pago o cuando el empleado utiliza una tarjeta de crédito para el pago de una compra personal."@es ,
                                                               "Type of billing scheme which occurs when an employee submits an invoice for personal purchases to the company for payment, or when an employee uses a company credit card for personal purchases."@en ;
                                                  rdfs:label "Compras personales"@es ,
                                                             "Personal Purchases"@en .


###  http://purl.org/ontology/onfraud#ConflictoDeInteres
<http://purl.org/ontology/onfraud#ConflictoDeInteres> rdf:type owl:Class ;
                                                      rdfs:subClassOf <http://purl.org/ontology/onfraud#Corrupción> ;
                                                      rdfs:comment "Situación en la que una persona u organización tiene intereses financieros o de otro tipo con terceros que podrían corromper la motivación o la toma de decisiones de ese individuo u organización."@en ,
                                                                   "Situation in which a person or organization is involved in multiple interests, financial or otherwise, one of which could possibly corrupt the motivation or decision-making of that individual or organization."@en ;
                                                      rdfs:label "Conflict of interest"@en ,
                                                                 "Conflicto de Interés"@es .


###  http://purl.org/ontology/onfraud#ConnivenciaProveedor
<http://purl.org/ontology/onfraud#ConnivenciaProveedor> rdf:type owl:Class ;
                                                        rdfs:subClassOf <http://purl.org/ontology/onfraud#PagosFraudulentosEsquemaFacturación> ;
                                                        rdfs:comment "Este tipo de fraude ocurre cuando un empleado arregla el sobrepago de una factura de un proveedor y se queda con el excedente del coste real de la factura"@es ,
                                                                     "Type of billing scheme which occurs when an employee arranges for overpayment of a vendor invoice and pockets the overpayment amount when it is returned to the company."@en ;
                                                        rdfs:label "Connivencia con el proveedor"@es ,
                                                                   "Non-Acomplice Vendor"@en .


###  http://purl.org/ontology/onfraud#Corrupción
<http://purl.org/ontology/onfraud#Corrupción> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://purl.org/ontology/onfraud#FraudeInterno> ;
                                              rdfs:comment "A scheme in which an employee misuses his or her influence in a business transaction in a way that violates his or her duty to the employer in order to gain a direct or indirect benefit."@en ,
                                                           "El empleado hace un uso indebido de su influencia en una transacción comercial de una manera que viola su deber para con el empleador a fin de obtener un beneficio directo o indirecto, para él o para la organización"@es ;
                                              rdfs:label "Corrupción"@es ,
                                                         "Corruption"@en .


###  http://purl.org/ontology/onfraud#Defraudador
<http://purl.org/ontology/onfraud#Defraudador> rdf:type owl:Class ;
                                               rdfs:subClassOf <http://www.w3.org/2000/10/swap/pim/contact#Person> .


###  http://purl.org/ontology/onfraud#DefraudadorExterno
<http://purl.org/ontology/onfraud#DefraudadorExterno> rdf:type owl:Class ;
                                                      rdfs:subClassOf <http://purl.org/ontology/onfraud#Defraudador> ;
                                                      rdfs:comment "Organization or person that commits a fraud against another Agent"@en ,
                                                                   "Tercero que comete un fraude contra una persona u organización"@es ;
                                                      rdfs:label "Defraudador Externo"@es ,
                                                                 "External Fraudster"@en .


###  http://purl.org/ontology/onfraud#DefraudadorInterno
<http://purl.org/ontology/onfraud#DefraudadorInterno> rdf:type owl:Class ;
                                                      rdfs:subClassOf <http://purl.org/ontology/onfraud#Defraudador> ,
                                                                      <http://purl.org/ontology/onfraud#Empleado> ,
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty <http://purl.org/ontology/onfraud#causaPerjuicioEconomico> ;
                                                                        owl:someValuesFrom <http://purl.org/ontology/onfraud#Victima>
                                                                      ] ,
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty <http://purl.org/ontology/onfraud#worksIn> ;
                                                                        owl:someValuesFrom <http://purl.org/ontology/onfraud#Victima>
                                                                      ] ;
                                                      rdfs:comment "Empleado que comete un fraude contra su empresa"@es ,
                                                                   "Employee who commits fraud against its Employer"@en ;
                                                      rdfs:label "Defraudador Interno"@es ,
                                                                 "Internal Fraudster"@en .


###  http://purl.org/ontology/onfraud#DevolucionFalsa
<http://purl.org/ontology/onfraud#DevolucionFalsa> rdf:type owl:Class ;
                                                   rdfs:subClassOf <http://purl.org/ontology/onfraud#PagosFraudulentosRegistroDesembolso> ;
                                                   rdfs:comment "Se produce cuando un empleado registra una devolución sin que se esté produciendo"@es ,
                                                                "Type of register disbursement scheme which occurs when an employee processes a transaction as if a customer were returning merchandise, even though there is no actual return."@en ;
                                                   rdfs:label "Devoluciones falsas"@es ,
                                                              "False Refunds"@en .


###  http://purl.org/ontology/onfraud#Empleado
<http://purl.org/ontology/onfraud#Empleado> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://www.w3.org/2000/10/swap/pim/contact#Person> ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://purl.org/ontology/onfraud#worksIn> ;
                                                              owl:someValuesFrom <http://www.w3.org/ns/org#Organization>
                                                            ] ;
                                            rdf:label "Empleado"@es ,
                                                      "Employee"@en .


###  http://purl.org/ontology/onfraud#EmpleadoFantasma
